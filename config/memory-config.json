{
  "reasoningBank": {
    "enabled": true,
    "dbPath": ".agentdb/reasoningbank.db",
    "dimension": 384,
    "quantization": "scalar",
    "cacheSize": 1500,
    "hnswM": 16,
    "hnswEfSearch": 100,
    "enableLearning": true,
    "enableReasoning": true,
    "optimizeMemory": true
  },
  "claudeFlow": {
    "memoryDb": ".swarm/memory.db",
    "enableHooks": true,
    "enableCoordination": true,
    "autoSave": true,
    "sessionPersistence": true
  },
  "namespaces": {
    "agents": "swarm/agent",
    "decisions": "project/decisions",
    "patterns": "reasoningbank/patterns",
    "trajectories": "reasoningbank/trajectories",
    "events": "system/events",
    "performance": "system/performance",
    "bugs": "system/bugs"
  },
  "performance": {
    "batchInserts": true,
    "parallelQueries": true,
    "cacheEnabled": true,
    "vacuumSchedule": "weekly"
  },
  "learning": {
    "safla": {
      "successMultiplier": 1.20,
      "failureMultiplier": 0.85,
      "minConfidence": 0.05,
      "maxConfidence": 0.95
    },
    "algorithms": [
      "decision-transformer",
      "q-learning",
      "sarsa",
      "actor-critic",
      "active-learning"
    ]
  }
}
